{
  "$schema": "http://json.schemastore.org/template",
  "author": "Matt Janda",
  "classifications": ["JandaBox/Package"],
  "name": "JandaBox NuGet File Package Solution",
  "generatorVersions": "[1.0.0.0-*)",
  "description": "JandaBox package project template creates a NuGet package that delivers files alongside compiled binaries.",
  "groupIdentity": "packagebox",
  "precedence": "4000",
  "identity": "JandaBox.Package",
  "shortName": "packagebox",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "PackageBox",
  "preferNameDirectory": true,
  "sources": [
    {
      "modifiers": [
        {
          "exclude": [
            "**/.vs/**",
            "**/bin/**",
            "**/obj/**",
            "**/.vscode/**"
          ]
        },
        {
          "condition": "(!git-version)",
          "exclude": [
            "GitVersion.yml"
          ]
        }
      ]
    }
  ],
  "symbols": {
    "sourceName": {
      "type": "parameter",
      "replaces": "PackageBox",
      "isEnabled": false
    },
    "git-version": {
      "type": "parameter",
      "datatype": "bool",
      "defaultValue": "false",
      "description": "Provide semantic versioning with GitVesion."
    },
    "projectTypeId":{
      "type": "generated",
      "generator": "guid",
      "replaces": "PROJECT_INSTANCE_GUID",
      "parameters": {
        "defaultFormat":"D"
      }
    },
    "solutionId":{
      "type": "generated",
      "generator": "guid",
      "replaces": "SOLUTION_GUID",
      "parameters": {
        "defaultFormat":"D"
      }
    }
  }
}