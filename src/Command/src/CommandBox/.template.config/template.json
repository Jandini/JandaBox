{
  "$schema": "http://json.schemastore.org/template",
  "author": "Matt Janda",
  "classifications": ["JandaBox/Command"],
  "name": "JandaBox Command Line Interface Project",
  "generatorVersions": "[1.0.0.0-*)",
  "description": "JandaBox Command provides project for console application with dependency injection, logging, and configuration.",
  "groupIdentity": "commandprj",
  "precedence": "4000",
  "identity": "JandaBox.Command.Project",
  "shortName": "commandprj",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "CommandBox",
  "preferNameDirectory": true,
  "sources": [
    {
      "modifiers": [
        {
          "exclude": [
            "**/.vs/**",
            "**/bin/**",
            "**/obj/**"
          ]
        },
        {
          "condition": "(basic)",
          "exclude": [
            "appsettings.json",
            "Extensions.cs",
            "Options.cs",
            "Settings.cs",
            "Properties/*",
            "Options/*"
          ]
        },
        {
          "condition": "(!options)",
          "exclude": [
            "Options/SettingsOptions.cs"
          ]
        },
        {
          "condition": "(nswag)",
          "exclude": [
            "nswag.json"
          ]
        },
        {
          "condition": "(!vscode)",
          "exclude": [            
            ".vscode/**"
          ]
        }
      ]
    }
  ],
  "symbols": {
    "sourceName": {
      "type": "parameter",
      "replaces": "CommandBox",
      "isEnabled": false
    },
    "vscode": {
      "type": "parameter",
      "datatype": "bool",
      "defaultValue": "false",
      "description": "Create launch.json and tasks.json files for Visual Studio Code."
    },
    "path": {
      "type": "parameter",
      "datatype": "text",
      "defaultValue": "",
      "replaces": "VS_PATH",
      "description": "Add relative path to tasks.json and launch.json files. e.g. 'src'"
    },
    "basic": {
      "type": "parameter",
      "datatype": "bool",
      "defaultValue": "false",
      "description": "Create basic console application."
    },
    "serilog": {
      "type": "parameter",
      "datatype": "bool",
      "defaultValue": "true",
      "description": "Use Serilog logger."
    },
    "options": {
      "type": "parameter",
      "datatype": "bool",
      "defaultValue": "false",
      "description": "Use IOptions pattern."
    },
    "async": {
      "type": "parameter",
      "datatype": "bool",
      "defaultValue": "true",
      "description": "Run main with async await."
    },
    "nswag": {
      "type": "parameter",
      "datatype": "bool",
      "defaultValue": "false",
      "description": "Add Web API client generator."
    },
    "class": {
      "type": "parameter",
      "datatype": "string",
      "replaces": "API_CLIENT",
      "defaultValue": "ApiClient",
      "description": "The class name for the Web API client generated by NSwag."
    },
    "url": {
      "type": "parameter",
      "datatype": "string",
      "replaces": "SWAGGER_JSON_URL",
      "defaultValue": "https://localhost:8080/swagger/v1/swagger.json",
      "description": "Swagger json URL or file path."
    },
    "baseurl": {
      "type": "parameter",
      "datatype": "string",
      "replaces": "BASE_URL",
      "defaultValue": "https://localhost:8080",
      "description": "Web API client base url."
    }
  }
}