{
  "$schema": "http://json.schemastore.org/template",
  "author": "Matt Janda",
  "classifications": ["JandaBox/Console"],
  "name": "JandaBox Console Application Solution",
  "generatorVersions": "[1.0.0.0-*)",
  "description": "JandaBox Console Application template provides console application with dependency injection, logging, and configuration.",
  "groupIdentity": "consolebox",
  "precedence": "4000",
  "identity": "JandaBox.Console",
  "shortName": "consolebox",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "ConsoleBox",
  "preferNameDirectory": true,
  "sources": [
    {
      "modifiers": [
        {
          "exclude": [
            "**/.vs/**",
            "**/bin/**",
            "**/obj/**",
            "src/ConsoleBox/.vscode/**"
          ]
        },
        {
          "condition": "(!vscode)",
          "exclude": [            
            ".vscode/**"
          ]
        },
        {
          "condition": "(!git-version)",
          "exclude": [
            "GitVersion.yml"
          ]
        },
        {
          "condition": "(!nswag)",
          "exclude": [
            "src/ConsoleBox/nswag.json"
          ]
        }
      ]
    }
  ],
  "symbols": {
    "sourceName": {
      "type": "parameter",
      "replaces": "ConsoleBox",
      "isEnabled": false
    },
    "vscode": {
      "type": "parameter",
      "datatype": "bool",
      "defaultValue": "false",
      "description": "Create launch.json and tasks.json files for Visual Studio Code."
    },
    "git-version": {
      "type": "parameter",
      "datatype": "bool",
      "defaultValue": "false",
      "description": "Provide semantic versioning with GitVesion."
    },
    "projectTypeId":{
      "type": "generated",
      "generator": "guid",
      "replaces": "PROJECT_INSTANCE_GUID",
      "parameters": {
        "defaultFormat":"D"
      }
    },
    "solutionId":{
      "type": "generated",
      "generator": "guid",
      "replaces": "SOLUTION_GUID",
      "parameters": {
        "defaultFormat":"D"
      }
    }
  }
}